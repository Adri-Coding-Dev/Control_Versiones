package iteradores;
import java.util.*;
public class Ejercicio_2 {
	public static void main(String[]args) {
		/*Crea una lista de números enteros. 
		 *Elimina usando un Iterator todos los números 
		 *menores a 10 y muestra la lista resultante.*/
		//50 Números (entre 1 y 50)
		int min=1,max=50;
		Scanner s=new Scanner(System.in);
		ArrayList<Integer>Numeros=new ArrayList<>();
		for(int i=0;i<50;i++) {
			int num=(int)Math.round(Math.random()*(max-min)-min);
			Numeros.add(num);
			if(num<10) {
				System.out.println("El elemento: "+num+" se ha borrado (x<10)");
				Numeros.remove(num);
				break;
			}
		}
		System.out.println("Numeros que son Mayores a 10: "+Numeros);
	}
}
